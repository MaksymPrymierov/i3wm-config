!#if __has_include(".extend.Xresources")
!#include "~/.resources/.extend.Xresources"
!#endif

! ^ The above lines are no comments!
! Leave them as they are if a file ~/.extend.Xresources is being used on your system.
! config can be added there or also here below.
! For comments use "!"

! special
*.foreground:   #dbd4e8
*.background:   #0c0026
*.cursorColor:  #dbd4e8

! black
*.color0:       #120b1b
*.color8:       #9fb36d

! red
*.color1:       #1e142b
*.color9:       #af95d5

! green
*.color2:       #322344
*.color10:      #d69c70

! yellow
*.color3:       #4d3968
*.color11:      #7ec5df

! blue
*.color4:       #705495
*.color12:      #85d0b7

! magenta
*.color5:       #947bb7
*.color13:      #b996ea

! cyan
*.color6:       #baabd1
*.color14:      #9db460

! white
*.color7:       #e5e0ee
*.color15:      #ea56a7

Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault


#background = #211324

XTerm*background: #211324
XTerm*foreground: #f6f9f2
XTerm*pointerColor: #16A085
XTerm*faceName: Fixed
XTerm*faceSize: 11
XTerm.vt100.locale: true
XTerm.vt100.faceName: xft:Hack:size=10

! стиль полосы прокрутки - rxvt (по умолчанию), plain (самый компактный), next, или xterm
!URxvt.scrollBar: false

URxvt.internalBorder: 6
URxvt.font: xft:Hack:bold:size=10
URxvt.depth: 16
URxvt.background:	[100]#0C0026
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true
URxvt*foreground: 		                #dbd4e8
URxvt*cursorColor:		                #dbd4e8
URxvt*skipBuiltinGlyphs:    true
